            android:name="com.icon256bliss.android.coursehandler.ProcessedChoicesDetailActivity"
            android:label="@string/title_processedchoices_detail"
            android:parentActivityName="com.icon256bliss.android.coursehandler.ProcessedChoicesListActivity"
